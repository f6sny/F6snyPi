{
  "name": "@6degrees/f6snypi",
  "version": "1.0.8",
  "description": "An API wrapper for https://api.f6sny.com for NPM",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --coverage",
    "clean": "run-script-os",
    "clean:darwin:linux": "rm -rf ./lib",
    "clean:win32": "rmdir /s /q lib",
    "build": "npm run clean && npm run build:esm & npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && node -e \"require('fs').rename('lib/esm/index.js', 'lib/esm/index.mjs', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f6sny/F6snyPi.git"
  },
  "keywords": [
    "api",
    "f6sny",
    "jokes",
    "npm",
    "package",
    "arabic"
  ],
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "author": "6 Degrees Technologies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/f6sny/F6snyPi/issues"
  },
  "homepage": "https://github.com/f6sny/F6snyPi#readme",
  "devDependencies": {
    "jest": "^29.5.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "run-script-os": "^1.1.6"
  }
}
